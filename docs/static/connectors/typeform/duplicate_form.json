{
  "id": "Fn05U3T6GP2A",
  "callback_id": "duplicate_form",
  "title": "Duplicate an existing form",
  "description": "Creates copy of a form with updated title",
  "type": "app",
  "input_parameters": [
    {
      "type": "string",
      "name": "form_id",
      "description": "Select a form",
      "title": "Form to duplicate",
      "is_required": true,
      "dynamic_options": {
        "function": "#/functions/list_forms",
        "inputs": {
          "search_query": {
            "value": "{{client.query}}"
          },
          "typeform_access_token": {
            "value": "{{input_parameters.typeform_access_token}}"
          }
        },
        "options": {
          "target_array": {
            "type": "object",
            "value": "{{output_parameters.forms}}"
          },
          "value_path": "id",
          "label_path": "title"
        }
      }
    },
    {
      "type": "string",
      "name": "form_title",
      "description": "Enter text",
      "title": "Name for new form",
      "is_required": true
    },
    {
      "type": "string",
      "name": "workspace",
      "description": "Select a workspace",
      "title": "Workspace",
      "is_required": false,
      "hint": "The selection is workspace href",
      "dynamic_options": {
        "function": "#/functions/list_workspaces",
        "inputs": {
          "search_query": {
            "value": "{{client.query}}"
          },
          "typeform_access_token": {
            "value": "{{input_parameters.typeform_access_token}}"
          }
        },
        "options": {
          "target_array": {
            "type": "object",
            "value": "{{output_parameters.workspaces}}"
          },
          "value_path": "self.href",
          "label_path": "name"
        }
      }
    },
    {
      "type": "slack#/types/credential/oauth2",
      "name": "typeform_access_token",
      "title": "Typeform access token",
      "is_required": true,
      "oauth2_provider_key": "typeform"
    }
  ],
  "output_parameters": [
    {
      "type": "string",
      "name": "form_id",
      "title": "Form ID",
      "is_required": false
    },
    {
      "type": "string",
      "name": "form_title",
      "title": "Form name",
      "is_required": false
    },
    {
      "type": "string",
      "name": "form_url",
      "title": "Form display URL",
      "is_required": false
    }
  ],
