{
  "id": "Fn05V8NGEZ7T",
  "callback_id": "create_folder",
  "title": "Create a folder",
  "description": "",
  "type": "app",
  "input_parameters": [
    {
      "type": "string",
      "name": "name",
      "description": "Enter folder name",
      "title": "Folder name",
      "is_required": true
    },
    {
      "type": "string",
      "name": "parent_path",
      "description": "Select parent folder",
      "title": "Parent folder",
      "is_required": false,
      "dynamic_options": {
        "function": "#/functions/search_files",
        "inputs": {
          "query": {
            "value": "{{client.query}}"
          },
          "file_category": {
            "value": "folder"
          },
          "dropbox_access_token": {
            "value": "{{input_parameters.dropbox_access_token}}"
          }
        },
        "request_config": {
          "search": {
            "min_query_length": 1
          }
        },
        "options": {
          "target_array": {
            "type": "object",
            "value": "{{output_parameters.matches}}"
          },
          "value_path": "metadata.metadata.path_lower",
          "label_path": "metadata.metadata.name"
        },
        "validation": {
          "function": "#/functions/get_file",
          "inputs": {
            "path": {
              "value": "{{input_parameters.parent_path}}"
            },
            "dropbox_access_token": {
              "value": "{{input_parameters.dropbox_access_token}}"
            }
          },
          "options": {
            "target": {
              "type": "object",
              "value": "{{output_parameters.file}}"
            },
            "value_path": "path_lower",
            "label_path": "name"
          }
        }
      }
    },
    {
      "type": "slack#/types/credential/oauth2",
      "name": "dropbox_access_token",
      "title": "Dropbox access token",
      "is_required": true,
      "oauth2_provider_key": "dropbox",
      "require_end_user_auth": true
    }
  ],
  "output_parameters": [
    {
      "type": "string",
      "name": "id",
      "description": "Id of the folder",
      "title": "Folder ID",
      "is_required": false
    },
    {
      "type": "string",
      "name": "name",
      "description": "Folder name",
      "title": "Name",
      "is_required": false
    },
    {
      "type": "string",
      "name": "path_display",
      "description": "Display path",
      "title": "Display path",
      "is_required": false
    }
  ],
