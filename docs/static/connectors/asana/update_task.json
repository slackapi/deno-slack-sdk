{
  "id": "Fn05ND9R23QB",
  "callback_id": "update_task",
  "title": "Update a task",
  "description": "",
  "type": "app",
  "input_parameters": [
    {
      "type": "string",
      "name": "workspace_gid",
      "description": "Workspace GID",
      "title": "Workspace",
      "is_required": true,
      "dynamic_options": {
        "function": "#/functions/get_workspaces",
        "inputs": {
          "asana_access_token": {
            "value": "{{input_parameters.asana_access_token}}"
          }
        },
        "options": {
          "target_array": {
            "type": "object",
            "value": "{{output_parameters.asana_workspaces}}"
          },
          "value_path": "gid",
          "label_path": "name"
        }
      }
    },
    {
      "type": "string",
      "name": "task_gid",
      "title": "Task to update",
      "is_required": true,
      "dynamic_options": {
        "function": "#/functions/get_tasks",
        "inputs": {
          "task_name": {
            "value": "{{client.query}}"
          },
          "workspace_gid": {
            "value": "{{input_parameters.workspace_gid}}"
          },
          "asana_access_token": {
            "value": "{{input_parameters.asana_access_token}}"
          }
        },
        "options": {
          "target_array": {
            "type": "object",
            "value": "{{output_parameters.asana_tasks}}"
          },
          "value_path": "gid",
          "label_path": "name"
        }
      }
    },
    {
      "type": "slack#/types/user_id",
      "name": "assignee",
      "description": "Select a user",
      "title": "New assignee",
      "is_required": false,
      "hint": "If the user you select isn't in the Asana workspace yet, they'll get an automatic email invite to join."
    },
    {
      "type": "string",
      "name": "name",
      "description": "Enter text",
      "title": "New task name",
      "is_required": false,
      "default": ""
    },
    {
      "type": "slack#/types/timestamp",
      "name": "start_at",
      "title": "New start at date",
      "is_required": false,
      "hint": "Ensure to set a due date as well; applicable for paid plans."
    },
    {
      "type": "slack#/types/timestamp",
      "name": "due_at",
      "title": "New due at date",
      "is_required": false
    },
    {
      "type": "string",
      "name": "description",
      "description": "Enter text",
      "title": "New description",
      "is_required": false,
      "long": true,
      "default": ""
    },
    {
      "type": "boolean",
      "name": "completed",
      "title": "Mark complete",
      "is_required": false,
      "hint": "Choose whether to mark this task as complete in Asana.",
      "default": false
    },
    {
      "type": "slack#/types/credential/oauth2",
      "name": "asana_access_token",
      "description": "Asana credential to use",
      "title": "Asana Access Token",
      "is_required": true,
      "oauth2_provider_key": "asana"
    }
  ],
  "output_parameters": [
    {
      "type": "string",
      "name": "assignee",
      "description": "The name of the user assigned to the task",
      "title": "Assignee of task",
      "is_required": false,
      "nullable": true
    },
    {
      "type": "string",
      "name": "task_name",
      "description": "The name of the task",
      "title": "Task name",
      "is_required": true
    },
    {
      "type": "string",
      "name": "start_at",
      "description": "The datetime the task is to be started on",
      "title": "Start at",
      "is_required": false,
      "nullable": true
    },
    {
      "type": "string",
      "name": "due_at",
      "description": "The datetime the task is due at",
      "title": "Due at",
      "is_required": false,
      "nullable": true
    },
    {
      "type": "string",
      "name": "description",
      "description": "The description for the task",
      "title": "Description",
      "is_required": false,
      "nullable": true
    },
    {
      "type": "string",
      "name": "created_at",
      "description": "The datetime the task was created at",
      "title": "Created at",
      "is_required": false,
      "nullable": true
    },
    {
      "type": "boolean",
      "name": "completed",
      "description": "Indicates task completion status",
      "title": "Is Task Completed",
      "is_required": false
    },
    {
      "type": "string",
      "name": "permalink_url",
      "description": "Link for the created Task",
      "title": "Link to task",
      "is_required": true,
      "format": "url"
    }
  ],
